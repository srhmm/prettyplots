

%%%%%%%%%%%%
%% Axis types

%% Line, Scatter

\pgfplotsset{
	%
	% #1 = x, y, or z
	% #2 = the shift value
	/tikz/normal shift/.code 2 args = {%
		\pgftransformshift{%
			\pgfpointscale{#2}{\pgfplotspointouternormalvectorofticklabelaxis{#1}}%
		}%
	},%
	%
	pretty axis/.style={
		axis lines*       	= left,
		tick style    		= {thin, black, major tick length=2pt},
		x tick label style 	= {font=\scriptsize, yshift = 1pt},
		y tick label style 	= {font=\scriptsize, xshift = 1pt},
		x label style 		= {at={($(axis description cs:0.5,0) + (0,-15pt)$)}, anchor=north, font=\scriptsize},
		y label style 		= {at={($(axis description cs:0,0.5) + (-20pt,0)$)}, anchor=south, font=\scriptsize},	
		x tick scale label style={at={(axis description cs:1,0)},xshift=1.5em}, 
		y tick scale label style={at={(axis description cs:0,1)},yshift=0.5em, xshift=-.5em}, 
		legend cell align 	= left,
		legend style 		= {inner sep = 1pt, cells = {font=\scriptsize}, },
		legend image code/.code={%
			\draw[mark repeat=2,mark phase=2,#1] 
			plot coordinates { (0cm,0cm) (0.15cm,0cm) (0.3cm,0cm) };% 
		}
	}
}


%% Bar 

\pgfplotsset{ 
	pretty ybar axis/.style={
		pretty axis,
		area legend,
		ymajorgrids,
		no markers,
		axis on top,
		xtick				= data,
		%cycle list name    	= mamba-bar,
		tick align        	= outside,
		enlargelimits     	= false,
		xmajorgrids 		= false,
		bar width			= 0.4em,
		major grid style	= white,
		axis lines* 		= left,			% only show two lines of the bounding box
		tick style    		= {thin, black, major tick length=2pt},
		major y tick style	= {draw=none},
		x axis line style 	= {thick, normal shift={x}{0pt}},
		y axis line style	= {opacity=0},	% but make y-axis invisible
		x tick label style 	= {font=\scriptsize, yshift = -1pt},
		y tick label style 	= {font=\scriptsize, xshift = -1pt},	
	}
}

\pgfplotsset{
	pretty xbar axis/.style={
		pretty axis,
		area legend,
		axis on top,
		tick align        	= outside,
		enlarge y limits 	= 0.3, %or true
		bar width 			= 10pt, 
	}
}

%% 3d

\pgfplotsset{
	every axis/.append style = {thick},tick style = {thick,black},
	%
	% #1 = x, y, or z
	% #2 = the shift value
	/tikz/normal shift/.code 2 args = {%
		\pgftransformshift{%
			\pgfpointscale{#2}{\pgfplotspointouternormalvectorofticklabelaxis{#1}}%
		}%
	},%
	%
	pretty 3daxis/.style = {
		tick align        = outside,
		scaled ticks      = false,
		enlargelimits     = false,
		ticklabel shift   = {10pt},
		axis lines*       = left,
		line cap          = round,
		clip              = false,
		xtick style       = {normal shift={x}{10pt}},
		ytick style       = {normal shift={y}{10pt}},
		ztick style       = {normal shift={z}{10pt}},
		x axis line style = {normal shift={x}{10pt}},
		y axis line style = {normal shift={y}{10pt}},
		z axis line style = {normal shift={z}{10pt}},
	}
} 

